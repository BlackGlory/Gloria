{
  "name": "Enhanced Notification Demo"
, "version": "1.0.0"
, "manifest_version": 2
, "options_ui": {
    "page": "options.html"
  , "chrome_style": true
  }
, "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
, "permissions": [
